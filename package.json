{
  "name": "@reyaxyz/ui-minions",
  "version": "1.0.1",
  "description": "Library that takes care of providing utility methods across Reya projects.",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/types",
  "type": "module",
  "directories": {
    "doc": "docs"
  },
  "files": [
    "src/**/*.*",
    "dist/**/*.*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "yarn code-quality:check && rm -rf dist",
    "build": "yarn prebuild && rollup -c --bundleConfigAsCjs",
    "ts:check": "yarn ts:check-types && yarn ts:circular-check",
    "ts:check-types": "tsc --noEmit",
    "ts:circular-check": "dpdm --no-output --no-tree --no-warning --exit-code circular:1 -T ./src/index.ts",
    "lint:check": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "prettier:check": "prettier --log-level silent --check './src/**/*.{ts,tsx}'",
    "prettier:fix": "prettier --log-level silent --write './src/**/*.{ts,tsx}'",
    "prepare": "is-ci || husky install",
    "release": "semantic-release",
    "test": "jest --silent",
    "test:coverage": "yarn test --coverage",
    "generate:coverage-badges": "npx istanbul-badges-readme --silent",
    "code-quality:check": "npx concurrently 'yarn ts:check' 'yarn prettier:check' 'yarn lint:check --quiet'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Reya-Labs/ui-minions.git"
  },
  "author": "antoni0dev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Reya-Labs/ui-minions/issues"
  },
  "homepage": "https://github.com/Reya-Labs/ui-minions#readme",
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.23.3",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "11.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "9.0.1",
    "@swc/core": "1.3.29",
    "@swc/jest": "0.2.24",
    "@tanstack/react-query": "5.36.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.1.0",
    "@testing-library/user-event": "14.5.1",
    "@types/jest": "29.4.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "17.0.18",
    "babel-loader": "8.3.0",
    "brokoli-ui": "8.37.0",
    "camelcase": "6.3.0",
    "concurrently": "7.6.0",
    "dpdm": "3.11.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "3.2.0",
    "eslint-plugin-unused-imports": "3.1.0",
    "ethers": "6.10.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "istanbul-badges-readme": "1.8.4",
    "jest": "29.6.2",
    "jest-environment-jsdom": "29.6.2",
    "prettier": "3.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "3.11.0",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-bundle-size": "1.0.3",
    "rollup-plugin-dts": "5.1.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-terser": "7.0.2",
    "semantic-release": "19.0.5",
    "semantic-release-cli": "5.4.4",
    "typescript": "5.4.2",
    "url-loader": "4.1.1",
    "viem": "2.10.9",
    "wagmi": "2.9.2"
  },
  "resolutions": {
    "underscore.string@~2.2.0rc": "2.2.1"
  },
  "peerDependencies": {
    "@tanstack/react-query": "5.37.1",
    "ethers": "6.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "viem": "2.10.9",
    "wagmi": "2.9.2"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "build",
              "release": "patch"
            },
            {
              "type": "chore",
              "release": "patch"
            },
            {
              "type": "ci",
              "release": "patch"
            },
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "feat",
              "release": "minor"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "perf",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "revert",
              "release": "patch"
            },
            {
              "type": "style",
              "release": "patch"
            },
            {
              "type": "test",
              "release": "patch"
            },
            {
              "type": "breaking",
              "release": "major"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "README.md"
          ],
          "message": "chore: Release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "engines": {
    "node": ">= 16.17.0",
    "npm": "please-use-yarn"
  },
  "packageManager": "yarn@4.1.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
